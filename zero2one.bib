@article{ ISI:000271620900004,
Author = {Guo, Qing-lin and Zhang, Ming},
Title = {{An agent-oriented approach to resolve scheduling optimization in
   intelligent manufacturing}},
Journal = {{ROBOTICS AND COMPUTER-INTEGRATED MANUFACTURING}},
