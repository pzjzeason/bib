@article{ ISI:000283559900291,
Author = {Kim, A-Rong and Kim, Gyeong-Hun and Kim, Kwang-Min and Kim, Jin-Geun and
   Kim, Dae-Won and Park, Minwon and Yu, In-Keun and Kim, Seok-Ho and Sim,
   Kideok and Seong, Ki-Chul},
Title = {{Design and Manufacturing of a SMES Model Coil for Real Time Digital
   Simulator Based Power Quality Enhancement Simulation}},
Journal = {{IEEE TRANSACTIONS ON APPLIED SUPERCONDUCTIVITY}},
