Title = {{Intelligent systems for the prognosis of energy consumption in
   manufacturing and assembly}},
Booktitle = {{9TH CIRP CONFERENCE ON INTELLIGENT COMPUTATION IN MANUFACTURING
   ENGINEERING - CIRP ICME `14}},
Series = {{Procedia CIRP}},
