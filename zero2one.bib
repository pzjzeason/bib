@inproceedings{ ISI:000391216300058,
Author = {Welo, Torgeir},
Editor = {{Emmanouilidis, C and Taisch, M and Kiritsis, D}},
Title = {{Intelligent Manufacturing Systems: Controlling Elastic Springback in
   Bending}},
Booktitle = {{ADVANCES IN PRODUCTION MANAGEMENT SYSTEMS: COMPETITIVE MANUFACTURING FOR
   INNOVATIVE PRODUCTS AND SERVICES, AMPS 2012, PT I}},
Series = {{IFIP Advances in Information and Communication Technology}},
Year = {{2013}},
