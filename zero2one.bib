Title = {{Real-time Production Scheduling Method for Automobile Engine
   Remanufacturing Based on the Internet of Manufacturing Things}},
Booktitle = {{PROCEEDINGS OF THE 11TH INTERNATIONAL CONFERENCE ON INNOVATION AND
   MANAGEMENT, VOLS I AND II}},
Year = {{2014}},
