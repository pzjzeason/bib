@inproceedings{ ISI:000394214800064,
Author = {Wieland, Matthias and Leymann, Frank and Schaefer, Michael and Lucke,
   Dominik and Constantinescu, Carmen and Westkaemper, Engelbert},
Editor = {{Mauri, JL and Balandin, S and Dini, C}},
Title = {{Using Context-aware Workflows for Failure Management in a Smart Factory}},
