@inproceedings{ ISI:000323630300048,
Author = {Fujii, Susumu and Fujii, Nobutada and Tsumaya, Akira and Iwamura, Koji
   and Morinaga, Eiji and Inoue, Tatsuo and Mariyama, Toshisada},
