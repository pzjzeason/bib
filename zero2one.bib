@article{ ISI:000285346300008,
Author = {Zhang, Yingfeng and Qu, T. and Ho, Oscar K. and Huang, George Q.},
Title = {{Agent-based Smart Gateway for RFID-enabled real-time wireless
   manufacturing}},
