@inproceedings{ ISI:000361249700085,
Author = {Zhang, Hao and Zhao, Fu and Sutherland, John W.},
Book-Group-Author = {{ASME}},
Title = {{MANUFACTURING SCHEDULING FOR ENERGY COST REDUCTION IN A SMART GRID
   SCENARIO}},
Booktitle = {{PROCEEDINGS OF THE ASME 9TH INTERNATIONAL MANUFACTURING SCIENCE AND
   ENGINEERING CONFERENCE, 2014, VOL 1}},
