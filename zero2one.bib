@article{ ISI:000341501800018,
Author = {Borangiu, Theodor and Raileanu, Silviu and Trentesaux, Damien and
   Berger, Thierry and Iacob, Iulia},
Title = {{Distributed manufacturing control with extended CNP interaction of
   intelligent products}},
