@inproceedings{ ISI:000376096100026,
Author = {Jafarinejad, Fatcmch and Pouvan, Ali A.},
Book-Group-Author = {{IEEE}},
Title = {{A Modular Synthesis Approach for Intelligent Manufacturing System Design
   a Petri net based Transformation Method}},
